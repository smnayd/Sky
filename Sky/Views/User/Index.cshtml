<table>
        <thead>
            <tr>
                <th>ID</th>
                <th>Username</th>
                <th>First Name</th>
                <th>Last Name</th>
                <th>Email</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var user in Model)
            {
                <tr>
                    <td>@user.Id</td>
                    <td>@user.Username</td>
                    <td>@user.Firstname</td>
                    <td>@user.Lastname</td>
                    <td>@user.Email</td>
                    <td>
                        @Html.ActionLink("Update", "Update", new { id = user.Id }) |
                        @Html.ActionLink("Delete", "Delete", new { id = user.Id })
                    </td>
                </tr>
            }
        </tbody>
    </table>

<p>
